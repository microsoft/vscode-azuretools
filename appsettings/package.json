{
    "name": "@microsoft/vscode-azext-azureappsettings",
    "author": "Microsoft Corporation",
    "version": "1.0.0",
    "description": "Common features and tools surrounding App Settings for the Azure VS Code extensions",
    "tags": [
        "azure",
        "vscode"
    ],
    "keywords": [
        "azure",
        "vscode"
    ],
    "module": "dist/esm/src/index.js",
    "main": "dist/cjs/src/index.js",
    "types": "dist/esm/src/index.d.ts",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/vscode-azuretools"
    },
    "bugs": {
        "url": "https://github.com/Microsoft/vscode-azuretools/issues"
    },
    "homepage": "https://github.com/Microsoft/vscode-azuretools/blob/main/appsettings/README.md",
    "scripts": {
        "build": "npm run build:esm && npm run build:cjs",
        "build:esm": "tsc -p ./",
        "build:cjs": "tsc -p ./ --outDir ./dist/cjs --module nodenext --moduleResolution nodenext --declaration false",
        "lint": "eslint --max-warnings 0",
        "test": "exit 0",
        "package": "npm pack",
        "l10n": "npx @vscode/l10n-dev export --outDir ./l10n ./src"
    },
    "engines": {
        "vscode": "^1.105.0"
    },
    "dependencies": {
        "@microsoft/vscode-azext-utils": "^4.0.0"
    },
    "devDependencies": {
        "@azure/arm-appservice": "^13.0.2",
        "@microsoft/vscode-azext-eng": "1.0.0-alpha.5",
        "@types/node": "22.x",
        "@types/vscode": "1.105.0"
    }
}
