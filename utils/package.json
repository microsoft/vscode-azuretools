{
    "name": "@microsoft/vscode-azext-utils",
    "author": "Microsoft Corporation",
    "version": "4.0.4",
    "description": "Common UI tools for developing Azure extensions for VS Code",
    "tags": [
        "azure",
        "vscode"
    ],
    "keywords": [
        "azure",
        "vscode"
    ],
    "module": "dist/esm/src/index.js",
    "main": "dist/cjs/src/index.js",
    "types": "index.d.ts",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/vscode-azuretools"
    },
    "bugs": {
        "url": "https://github.com/Microsoft/vscode-azuretools/issues"
    },
    "homepage": "https://github.com/Microsoft/vscode-azuretools/blob/main/utils/README.md",
    "scripts": {
        "build": "npm run build:esm && npm run build:cjs",
        "build:esm": "tsc -p ./",
        "build:cjs": "tsc -p ./ --outDir ./dist/cjs --module nodenext --moduleResolution nodenext --declaration false",
        "lint": "eslint --max-warnings 0",
        "test": "vscode-test",
        "package": "npm pack",
        "l10n": "npx @vscode/l10n-dev export --outDir ./l10n ./src"
    },
    "engines": {
        "vscode": "^1.105.0"
    },
    "dependencies": {
        "@microsoft/vscode-azureresources-api": "^3.1.0",
        "@vscode/extension-telemetry": "^1.5.0",
        "dayjs": "^1.11.19",
        "html-to-text": "^9.0.5",
        "semver": "^7.7.4",
        "vscode-tas-client": "^0.1.84"
    },
    "peerDependencies": {
        "@azure/ms-rest-azure-env": "^2.0.0"
    },
    "devDependencies": {
        "@microsoft/vscode-azext-eng": "1.0.0-alpha.12",
        "@types/html-to-text": "^9.0.4",
        "@types/node": "22.x",
        "@types/semver": "^7.7.1",
        "@types/vscode": "1.105.0",
        "@vscode/test-cli": "*",
        "@vscode/test-electron": "*"
    }
}
